      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {}
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ],
  "electronWebpack": {
    "renderer": {
      "sourceDirectory": "src",
      "webpackConfig": "dist_electron/webpack.renderer.additions.js"
    },
    "main": {
      "webpackConfig": "dist_electron/webpack.main.additions.js"
    }
