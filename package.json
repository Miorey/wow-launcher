    "open": "^7.4.2",
    "rimraf": "^3.0.2",
    "sound-play": "^1.1.0",
    "unzipper": "^0.10.11",
    "vue": "^2.7.14",
    "vue-i18n": "^8.28.2",
    "vuetify": "^2.6.14"
  },
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@babel/eslint-parser": "^7.19.1",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@vue/cli-plugin-babel": "^5.0.8",
    "@vue/cli-plugin-eslint": "^5.0.8",
